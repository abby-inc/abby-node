{
  "name": "@abby-inc/abby-node",
  "version": "1.0.0",
  "description": "Node.js library for the Abby API",
  "author": "Abby <contact@abby.fr>",
  "license": "MIT",
  "homepage": "https://github.com/abby-inc/abby-node",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/abby-inc/abby-node.git"
  },
  "bugs": {
    "url": "https://github.com/abby-inc/abby-node/issues"
  },
  "keywords": [
    "abby",
    "api",
    "sdk",
    "client",
    "invoicing",
    "freelance",
    "business"
  ],
  "main": "dist/cjs/index.cjs",
  "module": "dist/esm/index.js",
  "types": "dist/types/index.d.ts",
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.cjs"
    },
    "./package.json": "./package.json"
  },
  "packageManager": "pnpm@9.15.4",
  "scripts": {
    "generate": "openapi-ts",
    "build": "pnpm run generate && rollup -c rollup.config.mjs",
    "build:only": "rollup -c rollup.config.mjs",
    "clean": "rm -rf dist",
    "lint": "eslint src test --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "prepare": "husky",
    "prepublishOnly": "pnpm run clean && pnpm run build",
    "commitlint": "commitlint"
  },
  "dependencies": {
    "@hey-api/client-fetch": "~0.10.0",
    "zod": "^3.25.64"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.0.0",
    "@commitlint/config-conventional": "^19.0.0",
    "@eslint/js": "^9.39.2",
    "@hey-api/openapi-ts": "~0.89.2",
    "@rollup/plugin-commonjs": "^28.0.9",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^15.3.1",
    "@rollup/plugin-replace": "^6.0.2",
    "@rollup/plugin-typescript": "^12.3.0",
    "@types/node": "^22.19.3",
    "@typescript-eslint/eslint-plugin": "^8.50.1",
    "@typescript-eslint/parser": "^8.50.1",
    "@vitest/coverage-v8": "^2.0.0",
    "eslint": "^9.39.2",
    "husky": "^9.0.0",
    "lint-staged": "^15.0.0",
    "prettier": "^3.0.0",
    "rollup": "^4.54.0",
    "tslib": "^2.8.1",
    "typescript": "^5.9.3",
    "vitest": "^2.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
